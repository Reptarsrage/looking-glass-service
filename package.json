{
  "name": "looking-glass-service",
  "productName": "The Looking-Glass Service",
  "version": "1.0.0",
  "description": "This service serves up content from across the web.",
  "main": "index.js",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reptarsrage/looking-glass-service.git"
  },
  "author": {
    "name": "Reptarsrage",
    "email": "reptarsrage@gmail.com",
    "url": "https://github.com/reptarsrage"
  },
  "bugs": {
    "url": "https://github.com/reptarsrage/looking-glass-service/issues"
  },
  "keywords": [
    "fastify",
    "typescript"
  ],
  "homepage": "https://github.com/reptarsrage/looking-glass-service#readme",
  "scripts": {
    "prestart": "yarn build",
    "start": "cross-env NODE_ENV=production node dist/index.js | ./node_modules/.bin/pino-colada",
    "dev": "cross-env NODE_ENV=development nodemon src/index.ts | ./node_modules/.bin/pino-colada",
    "prebuild": "rimraf ./dist",
    "build": "tsc -p tsconfig.build.json",
    "test": "cross-env NODE_ENV=test jest",
    "lint": "eslint --cache --format=pretty \"{src,test}/**/*.ts\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/glob": "7.1.4",
    "@types/got": "9.6.12",
    "@types/jest": "27.0.2",
    "@types/node": "14.17.21",
    "@types/pino": "6.3.11",
    "@types/supertest": "2.0.11",
    "@types/xml2js": "0.4.9",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-formatter-pretty": "4.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.2",
    "jest": "27.2.5",
    "lint-staged": "11.2.2",
    "nodemon": "2.0.13",
    "pino-colada": "2.2.0",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "supertest": "6.1.6",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "typescript": "4.4.3"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "camelize": "^1.0.0",
    "cheerio": "^1.0.0-rc.9",
    "decamelize-keys": "^1.1.0",
    "dotenv": "^10.0.0",
    "fastify": "^3.9.1",
    "fastify-compress": "^3.4.0",
    "fastify-cors": "^6.0.0",
    "fastify-helmet": "^5.0.3",
    "fastify-static": "^4.0.0",
    "glob": "^7.1.6",
    "got": "^11.8.1",
    "image-size": "^1.0.0",
    "js-sha256": "^0.9.0",
    "jspack": "^0.0.4",
    "moment": "^2.29.1",
    "pino": "^6.8.0",
    "uuid": "^8.3.2",
    "xml2js": "^0.4.23"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --cache --format=pretty",
      "prettier --write"
    ],
    "*.{md,json,yml}": [
      "prettier --write"
    ]
  },
  "devEngines": {
    "node": ">=15.x",
    "yarn": ">=1.x"
  }
}
